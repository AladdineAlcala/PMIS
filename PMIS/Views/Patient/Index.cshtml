<div class="row">
    <div class="col-lg">
        <div class="card" style="min-height: 530px;">
            <div class="card-header">
                <h3 class="card-title">Patients</h3>
            </div>
            <div class="card-body h-100">
                <div class="table-responsive">
                    <table class="table table-striped table-sm table-bordered compact nowrap" id="tablePatient">
                        <thead class="bg-cyan">
                        <tr>
                            <th>ID</th>
                            <th>FULLNAME</th>
                            <th>ADDRESS</th>
                            <th>GENDER</th>
                            <th>AGE</th>
                            <th>CONTACT (CEL) </th>
                            <th></th>
                        </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

@section scripts
{
    <script type="text/javascript">
        
        var patientProfile={
            getPatientlist:"@Url.Action("GetAllPatients","Patient")",
            createPatientProfile: "@Url.Action("CreatePatientProfile","Patient")",
            removePatientProfile:"@Url.Action("RemovePatientProfile","Patient")"

        }
    </script>

    @Scripts.Render("~/Scripts/js_Patient.js")
